				.col-lg-6
					+e.table-wrap
						table
							tr
								th(colspan="3") Беседки из поликарбоната
							+tr("Беседка 3х3",'м','от 25 200 руб.')
							+tr("Беседка 4х4",'м','от 44 800 руб.')
							+tr("Беседка 5х5",'м','от 70 000 руб.')
							+tr("Беседка 6х6",'м','от 99 800 руб.') 
						table
							tr
								th(colspan="3") Навесы для авто
							+tr("Навес для авто 4х5,5",'м','от 33 700 руб.')
