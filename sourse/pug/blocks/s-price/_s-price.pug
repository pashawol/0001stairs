mixin s-price()
	// start s-price
	+b.SECTION.s-price.section#s-price&attributes(attributes)
		.container 
			+b.section-title.text-center.ttu
				h2 Стоимость
			.row
				mixin tr(t1,t2,t3)
					tr
						td= t1
						td
							.text-center= t2
						td
							.text-right= t3
				.col-lg-6
					+e.table-wrap
						table
							tr
								th(colspan="3") По типу материала
							+tr("Навес из поликарбоната",'м2','от 2 600 руб.')
							+tr("Навес из профнастила",'м2','от 2 600 руб.')
							+tr("Навес из черепицы",'м2','от 2 800 руб.')
							+tr("Навес с мягкой кровли",'м2','от 2 900 руб.') 
						table
							tr
								th(colspan="3") По месту назначения
							+tr("Навес для автомобиля",'м2','от 2 800 руб.')
							+tr("Навес над крыльцом",'м2','от 5 000 руб.')
							+tr("Навес для террасы",'м2','от 2 800 руб.')
							+tr("Навес для беседки",'м2','от 2 800 руб.')
							+tr("Навес для мангала",'м2','от 5 000 руб.')
							+tr("Навес для барбекю",'м2','от 2 800 руб.')
							+tr("Навес над калиткой",'м2','от 5 000 руб.')
							+tr("Навес для бассейна",'м2','от 2 800 руб.')
							+tr("Качели с навесом",'м2','от 10 000 руб.')
