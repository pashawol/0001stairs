mixin s-type-of-materials-block()
	mixin btn()
		+e.btn.link-modal(href="#modal-call" data-order=`Скачать весь каталог цветов` data-title='Скачать весь каталог цветов' data-text="Оставьте заявку и наш менеджер отправит вам каталог цветов!" data-btn="Заказать каталог цветов")  СКАЧАТЬ ВЕСЬ КАТАЛОГ ЦВЕТОВ
	// start s-type-of-materials
	+b.SECTION.s-type-of-materials.section#s-type-of-materials&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 15 вариантов отделки 
			
			+e.row5.row
				-
					var nav= {
						"1-1":'Сосна',
						"1-2":'Лиственница',
						"1-3":'Бук',
						"1-4":'Дуб',
						"1-5":'Ясень',
						"1-6":'Плитка',
						"1-7":'Стекло',
						"1-8":'Ковка',
						"1-9":'Пластик',
						"1-10":'ЛДСП',
						"1-11":'Камень',
						"1-12":'Металл',
						"1-13":'Чечевичный лист',
						"1-14":'ПВЛ лист',
						"1-15":'Решетчатый настил',
					}
				each linkPatch, linkName in nav
					+e.col5.col-lg.col-md-3.col-sm-4.col-6
						+itmat(linkName, linkPatch)
					// /+e.col5.col-lg.col-md-3.col-sm-4.col-6
			+btn()
			h2 5 Вариантов перил
			.row
				-
					var nav= {
						"2-1":'Кованые',
						"2-2":'Нержавеющие',
						"2-3":'Стальные',
						"2-4":'Стеклянные',
						"2-5":'Деревянные',
					}
				each linkPatch, linkName in nav
					+e.col5.col-lg.col-md-3.col-sm-4.col-6
						+itmat(linkName, linkPatch)
					// /+e.col5.col-lg.col-md-3.col-sm-4.col-6
			+btn()
			block
			+btn()
	// end s-type-of-materials

mixin s-type-of-materials()
	+s-type-of-materials-block()
		h2 24 ВАРИАНТА ПОКРАСКИ
		h6 ДЕРЕВЯННЫЕ ВАРИАНТЫ
		+block1() 
		h6 ПОКРАСКА
		+block2() 
