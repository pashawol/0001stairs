mixin itmat(i,t)
	+e.item
		//- +pic(`s-type-of-materials-${i}.jpg`,'bg','lazy',0)
		picture
			img.lazy(src="#" data-src=`img/@2x/s-type-of-materials-${i}.jpg`, alt="")
		+e.item-title= t

